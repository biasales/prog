
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

typedef struct{
    char *nome;
    int idade;
    float salario;
    int cpf;
    char sexo;

}ficha;

int main()
{
    ficha r1,r2;


    r1.nome = "Mario Andrade";
    r1.idade = 23;
    r1.salario = 2;
    r1.cpf = 34256743;
    r1.sexo = 'M';

    scanf("%[^\n]",&r2.nome);
    scanf("%d",&r2.idade);
    scanf("%f",&r2.salario);
    scanf("%d",&r2.cpf);
    scanf("%[^\n]",&r2.sexo);

    printf("%s \n  %d \n %2.f  \n %d \n %c \n",r1.nome,r1.idade,r1.salario,r1.cpf,r1.sexo);
    printf("\n");
    printf("%s \n  %d \n %2.f  \n %d \n %c \n",r2.nome,r2.idade,r2.salario,r2.cpf,r2.sexo);

    if (r1.salario > r2.salario){
            printf("aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa");

        printf(" o maior salario eh %s \n  %d \n %2.f \n %d \n  %c \n ",r1.nome,r1.idade,r1.salario,r1.cpf,r1.sexo);
    }
    else {
        printf(" o maior salario eh %s \n %d \n %2.f \n  %d \n  %c \n ",r2.nome,r2.idade,r2.salario,r2.cpf,r2.sexo);
        printf("bbbbbbbb");
        printf("ccccccccccccccccccccccccc");


    }
    return 0;
}
